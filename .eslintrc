{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle":"off",
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak-style": [0," error "," windows "],
    "require-jsdoc": "off",
    "no-case-declarations": "off"
  }
}